<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.byy.dal.mapper.forum.InvitationPicMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="InvitationPic">
		<id column="id" property="id"/>
		<result column="created_at" property="createdAt"/>
		<result column="updated_at" property="updatedAt"/>
		<result column="version" property="version"/>
		<result column="archive" property="archive"/>
		<result column="invitation_id" property="invitationId"/>
		<result column="img_url" property="imgUrl"/>
		<result column="img_type" property="imgType"/>

    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
		ip.id,ip.created_at,ip.updated_at,ip.version,ip.archive,ip.invitation_id,ip.img_url,ip.img_type
    </sql>

</mapper>

