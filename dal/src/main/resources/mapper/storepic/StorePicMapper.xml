<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.byy.dal.mapper.storepic.StorePicMapper">

  <!-- 通用查询映射结果 -->
  <resultMap id="BaseResultMap" type="StorePic">
    <id column="id" property="id"/>
    <result column="created_at" property="createdAt"/>
    <result column="updated_at" property="updatedAt"/>
    <result column="version" property="version"/>
    <result column="archive" property="archive"/>
    <result column="store_id" property="storeId"/>
    <result column="image" property="image"/>

  </resultMap>

  <!-- 通用查询结果列 -->
  <sql id="Base_Column_List">
		sp.id,sp.created_at,sp.updated_at,sp.version,sp.archive,sp.store_id,sp.image
    </sql>

</mapper>

