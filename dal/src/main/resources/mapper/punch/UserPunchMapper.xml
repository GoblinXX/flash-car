<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.byy.dal.mapper.punch.UserPunchMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="UserPunch">
		<id column="id" property="id"/>
		<result column="created_at" property="createdAt"/>
		<result column="updated_at" property="updatedAt"/>
		<result column="version" property="version"/>
		<result column="archive" property="archive"/>
		<result column="user_id" property="userId"/>
		<result column="accumulated_days" property="accumulatedDays"/>
		<result column="punch_time" property="punchTime"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
		up.id,up.created_at,up.updated_at,up.version,up.archive,up.user_id,up.accumulated_days,up.punch_time
    </sql>

</mapper>

